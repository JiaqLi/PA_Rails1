<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Subject</th>
      <th>Enroll</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @courses.each do |course| %>
      <tr>
        <td><%= course.name %></td>
        <td><%= course.subject_id %></td>
        <td><%= link_to 'Enroll', enrollments_path(course_id: "#{course.subject_id}", user_id: "#{current_user.id}"), method: :post %></td>
      </tr>
    <% end %>
  </tbody>
</table>
